{
  "expo": {
    "name": "나와의 대화",
    "slug": "talkwithme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#DEE6FD"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "talkwith.me",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.jpg",
        "backgroundColor": "#DEE6FD"
      },
      "package": "talkwith.me",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1f17ad22-c3ea-4b04-a699-604ccc95a09e"
      }
    },
    "owner": "talkwithme"
  }
}
