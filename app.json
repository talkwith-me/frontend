{
  "expo": {
    "name": "나와의 대화",
    "slug": "나와의 대화",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#DEE6FD"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "talkwith.me"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.jpg",
        "backgroundColor": "#DEE6FD"
      },
      "package": "talkwith.me"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash.jpg"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
